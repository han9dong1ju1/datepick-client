target 'iosApp' do
  use_frameworks!
  platform :ios, '14.0'
  pod 'Firebase/Analytics'
  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'Firebase/RemoteConfig'
  pod 'Firebase/Crashlytics'
  pod 'data', :path => '../shared/data'
  pod 'domain', :path => '../shared/domain'
  pod 'client', :path => '../shared/client'
  pod 'firebase', :path => '../shared/firebase'
end