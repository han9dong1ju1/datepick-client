import java.lang.Long;

CREATE TABLE featuredEntity (
    id INTEGER AS Long NOT NULL PRIMARY KEY,
    title TEXT NOT NULL,
    description TEXT NOT NULL,
    photoUrl TEXT NOT NULL,
    cachedAt INTEGER AS Long NOT NULL
);

findAll:
SELECT * FROM featuredEntity;

insert:
INSERT INTO featuredEntity(id, title, description, photoUrl, cachedAt) VALUES ?;

deleteAll:
DELETE FROM featuredEntity;